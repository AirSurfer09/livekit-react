{
  "name": "airsurfer-livekit-react",
  "version": "1.0.0",
  "description": "A React package for integrating Convai's AI-powered voice assistants with LiveKit for real-time audio/video conversations",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish",
    "example:react": "cd examples/react && pnpm dev",
    "example:react:build": "cd examples/react && pnpm build",
    "examples:dev": "pnpm example:react",
    "examples:build": "pnpm example:react:build",
    "examples:install": "pnpm install && cd examples/react && pnpm install",
    "examples:clean": "pnpm clean && cd examples/react && pnpm clean"
  },
  "keywords": [
    "livekit",
    "convai",
    "react",
    "typescript",
    "ai",
    "voice-assistant",
    "video-chat",
    "real-time",
    "webrtc"
  ],
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/airsurfer-livekit-react.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/airsurfer-livekit-react/issues"
  },
  "homepage": "https://github.com/yourusername/airsurfer-livekit-react#readme",
  "dependencies": {
    "@livekit/components-react": "^2.9.3",
    "@livekit/components-styles": "^1.1.4",
    "framer-motion": "^11.18.0",
    "livekit-client": "^2.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@types/node": "^20.17.13",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.0",
    "typescript": "^5.7.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}